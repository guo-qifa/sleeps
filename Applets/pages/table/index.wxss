.userinfo-avatar {
    width: 150rpx;
    height: 150rpx;
    margin: 20rpx;
    border-radius: 50%;
    margin-top: 50rpx;
}
.container{
    padding: 0px;
}
.background{
    width: 100%;
    height: 300rpx;
    background-size:100% 100%;
    z-index: 1;
    position: fixed;
}
.line{
    height: 20rpx;
    background-color: #f5f8fa;
    margin-top: 160px;
}
.list{
    padding: 10px 5%;
    display: flex;
    justify-content:space-between;
    border-bottom:3rpx solid #e5e5e5;
    background-color: #ffffff;
    box-shadow: 0rpx 1rpx 0rpx 0rpx
    #f0f0f0;
    color: #1a1a1a;
}
.list image {
    width: 30rpx;
    height: 30rpx;
    padding-top: 20px;
}
.list input{
    padding-top: 25rpx;
    padding-left: 25rpx;


}
.btnStyle {
    background-image: linear-gradient(180deg,
    #64bbf3 0%,
    #4e78e8 100%),
    linear-gradient(
            #26cdda,
            #26cdda);
    background-blend-mode: normal,
    normal;
    border-radius: 45 rpx;
    letter-spacing: 5 rpx;
    color: #ffffff;
    font-size: 25 rpx;
    margin-bottom: 20px;
    float: left;
    padding: 8px 10px;
    width: 100px;
    height: 40px;
    font-weight: inherit;

}

.Btnlist{
    display: flex;
    justify-content:center;
    margin-top: 150px;
    font-family: SourceHanSansCN-Regular;
}
.time{
    border-right: 1px solid #d2d2d2;
    padding-right: 20px;
}
.time .tip_key{
    font-size: 30rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 3rpx;
    color: #333333;
}
.time .tip_val {

    font-size: 20rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 1 rpx;
    color: #999999;
}
.xt{
    text-align: center;
}
.xt .tip_key{
    font-size: 24rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #333333;
    margin-bottom: 10px;

}
.xt .tip_val{
    font-size: 32rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #008aff;
}

