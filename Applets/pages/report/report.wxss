/* 第二个框样式 */

.levelBox {
    margin: 3% auto;
    width: 90%;
    height: 167rpx;
    background-color: rgba(0, 169, 222, .33);
    border-radius: 28rpx;
    background: url(https://www.topsleep.net/static/10-kuang2.png) no-repeat;
    background-size: contain !important;
    position: relative;
}

.levelBox .levelInfo {
    width: 152rpx;
    height: 126rpx;
    position: absolute;
}

.levelBox .levelInfo ._bottomText {
    color: #fff;
    font-size: 19rpx;
    width: 100%;
    position: absolute;
    text-align: center;
    bottom: 8rpx;
}

.levelBox ._level {
    left: 134rpx;
    top: 19rpx;
}

.levelBox ._level ._value {
    width: 100%;
    height: 70%;
    top: 4rpx;
    position: absolute;
    text-align: center;
    color: #fff;
    font-size: 92rpx;
    line-height: 88rpx;
}

.levelBox ._level ._unit {
    width: 30rpx;
    color: #fff;
    font-size: 20rpx;
    position: absolute;
    bottom: 36rpx;
    left: 104rpx;
}

.levelBox ._level2 ._value {
    width: 100%;
    height: 70%;
    top: 4rpx;
    position: absolute;
    text-align: center;
    color: #fff;
    font-size: 56rpx;
    line-height: 88rpx;
}

.levelBox .codePic {
    left: 286rpx;
    top: 32rpx;
    width: 100rpx;
    height: 100rpx;
}

.levelBox ._level2 {
    left: 434rpx;
    top: 21rpx;
    width: 139rpx;
    height: 122rpx;

}


/* 第三个框样式 */
.goods {
    margin-top: 10%;
    width: 606rpx;
    height: 477rpx;
    background-color: rgba(0, 169, 222, .33);
    border-radius: 28rpx;
}

.goods .title {
    display: block;
    position: absolute;
    top: -86rpx;
    left: 0;
    right: 0;
    z-index: 999;
    margin: auto;
    width: 390rpx !important;
    color: #fff !important;
    padding: 5rpx 40rpx;
    border-radius: 26rpx;
    font-size: 26rpx;
    text-align: center;
    background: #00a9de;
    margin-top: 65rpx;
}

.goods .content {
    width: 100%;
    height: 350rpx;
    margin-top: 60rpx;
    height: 100%;
}

.page-section {
    height: 100%;
}

swiper {
    height: 100%;
}


.btn-left image {
    width: 45rpx;
    height: 45rpx;
    position: absolute;
    top: 50%;
    left: -58rpx;
}

.btn-right image {
    width: 45rpx;
    height: 45rpx;
    position: absolute;
    top: 50%;
    right: -58rpx;
}

.swiper-item {
    position: relative;
    height: 100%;
    display: flex;
    flex-direction: row;
}

.swiper-item .conLeft {
    position: relative;
    height: 100%;
    display: flex;
    flex: 1;
    margin:130rpx 50rpx 38rpx 50rpx;
    background: url(https://www.topsleep.net/static/10-kuang4.png) no-repeat;
    background-size: contain !important;
}

.swiper-item .conRight {
    position: relative;
    height: 100%;
    display: flex;
    flex: 1;
    margin: 5rpx;
    margin: 130rpx 50rpx 38rpx 50rpx;
    background: url(https://www.topsleep.net/static/10-kuang4.png) no-repeat;
    background-size: contain !important;
    background-repeat: no-repeat;
}

