/* 背景图片 */
.report-box {
    background: url(https://www.topsleep.net/static/2-bg.png) no-repeat;
    background-size: cover !important;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    overflow-x: hidden;
    overflow-y: auto;
}

/* 第一个框样式 */
.total .title {
    display: block;
    position: absolute;
    top: -86rpx;
    left: 0;
    right: 0;
    z-index: 999;
    margin: auto;
    width: 290rpx !important;
    color: #fff !important;
    padding: 5rpx 40rpx;
    border-radius: 26rpx;
    font-size: 29rpx;
    text-align: center;
    background: #00a9de;
    margin-top: 65rpx;
}

.infobox {
    position: relative;
    top: 85rpx;
    /* display: flex; */
    flex-direction: column;
    margin: 0 auto;
    width: 90%;
    height: 400rpx;
    background-color: rgba(0, 169, 222, .33);
    border-radius: 28rpx;
}

.infobox ._top {
    display: flex;
    flex-direction: row;
    /* flex: 1; */
    height: 100rpx;
}

.infobox ._top .basicInfo {
    position: relative;
    display: flex;
    flex: 1;
    flex-direction: row;
}

.infobox ._top .basicInfo .icon {
    position: absolute;
    top: 42rpx;
    left: 25rpx;
    justify-content: center;
    align-items: center;
    width: 12rpx;
    height: 12rpx;
    background-color: #00a9de;
}

.infobox ._top .basicInfo .name {
    display: flex;
    flex: 1;
    justify-content: flex-end;
    align-items: center;
    font-family: SourceHanSansCN-Regular;
    font-size: 19rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 33rpx;
    letter-spacing: 4rpx;
    color: #ffffff;
}

.infobox ._top .basicInfo .value {
    display: flex;
    flex: 2;
    justify-content: left;
    align-items: center;
    font-family: SourceHanSansCN-Regular;
    font-size: 19rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 33rpx;
    letter-spacing: 4rpx;
    color: #ffffff;
}

.infobox ._bottom {
    display: flex;
    flex: 3;
    padding: 3px 20rpx;
    white-space: space-around;
    flex-wrap: wrap;
}

.infobox ._bottom .info {
    font-size: 26rpx;
    color: #fff;
    text-align: center;
    width: 33%;
    position: relative;
}
.infobox ._bottom .info image {
    width: 100rpx;
    height: 100rpx;
}
.infobox ._bottom .info .b_value {
    position: absolute;
    color: #fff;
    font-size: 23rpx;
    top: 37rpx;
    left: 85rpx;
}

