.userinfo-avatar {
    width: 150rpx;
    height: 150rpx;
    margin: 20rpx;
    border-radius: 50%;
    margin-top: 50rpx;
}
.container{
    padding: 0px;
}
.background{
    width: 100%;
    height: 40%;
    background-size:100% 100%;
    z-index: 1;
    position: fixed;
    margin-top: -75px;

}
.line{
    height: 50rpx;
    background-color: #f5f8fa;
    margin-top: 30px;
}
.list{
    padding: 15px 5%;
    display: flex;
    justify-content:space-between;
    border-bottom:3rpx solid #e5e5e5;
    background-color: #ffffff;
    box-shadow: 0rpx 1rpx 0rpx 0rpx
    #f0f0f0;
    color: #1a1a1a;
    font-family: SourceHanSansCN-Normal;

}
.list image {
    width: 30rpx;
    height: 30rpx;
}
.list input{
    padding-top: 25rpx;
    padding-left: 25rpx;


}
.btnStyle {
    background-image: linear-gradient(180deg,
    #64bbf3 0%,
    #4e78e8 100%),
    linear-gradient(
            #26cdda,
            #26cdda);
    background-blend-mode: normal,
    normal;
    border-radius: 45 rpx;
    letter-spacing: 5 rpx;
    color: #ffffff;
    font-size: 25 rpx;
    margin-bottom: 20px;
    float: left;
    padding: 8px 10px;
    width: 100px;
    height: 40px;
    font-weight: inherit;

}



.time .tip_key{
    font-size: 40rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 3rpx;
    color: #333333;
    margin-bottom: 10px;
}
.tip_key{
    font-size: 28rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #1a1a1a;
}
.tip_val{
    font-size: 28rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #008aff;
}

