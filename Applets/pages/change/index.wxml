<view class="changebox">
    <image class='background' src="{{imgUrl}}/centerbg.png"></image>
    <form bindsubmit='formSubmit'>
        <view class="wrap">
            <view class="list" bindtap="jumForm">
                <view>真实姓名:</view>
                <view>
                    <input type='text' name="username" value='' placeholder='请输入您的姓名' />
                </view>
            </view>
            <view class="list">
                <view>手机号码:</view>
                <view>
                    <input type='number' name="tel" value='' placeholder='请输入您的手机号' />

                </view>
            </view>
        </view>
        <view class="Btnlist">
            <!-- <button class="btnStyle" style="width: 120px;margin-right: 15px;border-radius: 50px;">修改</button> -->
            <button class="btnStyle" form-type='submit'
                style="width: 120px;border-radius: 50px;">保存</button>
        </view>
    </form>
</view>