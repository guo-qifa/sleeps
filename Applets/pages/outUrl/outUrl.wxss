.report-box {
    background: #1d2b41;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    overflow-x: hidden;
    overflow-y: auto;
}

.report-box .textimg {
    width: 100%;
    margin-top: 70rpx;
}

.report-box .toptext text {
    color: #fff;
}

.report-box .text-l {
    position: absolute;
    top: 110rpx;
    left: 170rpx;
    font-size: 200rpx;
}

.report-box .text-r {
    position: absolute;
    top: 160rpx;
    left: 455rpx;
    font-size: 130rpx;
}

.report-box .infobox {
    padding: 80rpx 30rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-around;
}

.report-box .infobox .body {
    width: 300rpx;
}

.report-box .infobox .infowrap {
    width: 350rpx;
    padding-left: 50rpx;
}

.report-box .infobox .infowrap .nickname {
    display: block;
    width: 100%;
    color: #fff;
    height: 40rpx;
    line-height: 40rpx;
}

.report-box .infobox .infowrap .itembox {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    margin-top: 20rpx;
}

.report-box .infobox .infowrap .item {
    width: 150rpx;
    height: 200rpx;
    position: relative;
    margin-top: 20rpx;
}

.report-box .infobox .infowrap .item .circular {
    width: 150rpx;
    height: 150rpx;
}

.report-box .infobox .infowrap .item .title {
    position: absolute;
    top: 58rpx;
    left: 0;
    color: #fff;
    font-size: 24rpx;
    width: 100%;
    text-align: center;
}

.report-box .infobox .infowrap .item .value {
    color: #fff;
    font-size: 30rpx;
    width: 100%;
    text-align: center;
    display: block;
    position: absolute;
    bottom: 8rpx;
}

.report-box .logo {
    display: flex;
    justify-content: center;
}

.report-box .logo image {
    width: 350rpx;
}